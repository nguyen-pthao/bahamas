<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/bahamas</property>
    <property name="hibernate.connection.username">root</property>
    <mapping resource="hibernate/Notice.hbm.xml"/>
    <mapping resource="hibernate/TeamAffiliationList.hbm.xml"/>
    <mapping resource="hibernate/Address.hbm.xml"/>
    <mapping resource="hibernate/EventLocationList.hbm.xml"/>
    <mapping resource="hibernate/PermissionLevelList.hbm.xml"/>
    <mapping resource="hibernate/PaymentModeList.hbm.xml"/>
    <mapping resource="hibernate/LsaClassList.hbm.xml"/>
    <mapping resource="hibernate/Donation.hbm.xml"/>
    <mapping resource="hibernate/RoleList.hbm.xml"/>
    <mapping resource="hibernate/TypeOfContactList.hbm.xml"/>
    <mapping resource="hibernate/Event.hbm.xml"/>
    <mapping resource="hibernate/MembershipClassList.hbm.xml"/>
    <mapping resource="hibernate/OfficeList.hbm.xml"/>
    <mapping resource="hibernate/LanguageAssignment.hbm.xml"/>
    <mapping resource="hibernate/TeamJoin.hbm.xml"/>
    <mapping resource="hibernate/OfficeHeld.hbm.xml"/>
    <mapping resource="hibernate/EventAffiliation.hbm.xml"/>
    <mapping resource="hibernate/Email.hbm.xml"/>
    <mapping resource="hibernate/Membership.hbm.xml"/>
    <mapping resource="hibernate/EventClassList.hbm.xml"/>
    <mapping resource="hibernate/LanguageList.hbm.xml"/>
    <mapping resource="hibernate/TrainingCertificated.hbm.xml"/>
    <mapping resource="hibernate/Proxy.hbm.xml"/>
    <mapping resource="hibernate/ModeOfSendingReceiptList.hbm.xml"/>
    <mapping resource="hibernate/SkillAssignment.hbm.xml"/>
    <mapping resource="hibernate/EventParticipant.hbm.xml"/>
    <mapping resource="hibernate/Appreciation.hbm.xml"/>
    <mapping resource="hibernate/Contact.hbm.xml"/>
    <mapping resource="hibernate/Phone.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
